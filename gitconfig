[alias]
	co = checkout
    cobt = checkout -b tmp

	st = status

	br = branch
    brdt = branch -d tmp
    brDt = branch -D tmp

	fe = fetch
	cm = commit
	ps = push
	pl = pull
	df = diff
    rs = reset

    rb = rebase
    rbm = rebase master
    rbom = rebase origin/master

    sth = stash
[credential]
	helper = cache
[fetch]
	prune = true
