[user]
    name = macinjoke
    email = shunji.makino@gmail.com
[color]
    ui = auto
[core]
    excludesfile = ~/.gitignore
[push]
    default = simple
[alias]
    co = checkout
    cobt = checkout -b tmp

    st = status

    br = branch
    brdt = branch -d tmp
    brDt = branch -D tmp
    brdm = !git branch -d $(git br --merged |grep -v master |grep -v '*')

    bw = browse
    fe = fetch
    cm = commit
    ps = push
    pl = pull
    df = diff
    rs = reset

    rb = rebase
    rbm = rebase master
    rbom = rebase origin/master

    sth = stash
    pbcopy = !printf "$1" | pbcopy
[credential]
    helper = cache
[fetch]
    prune = true
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
